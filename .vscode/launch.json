{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Acquire",
      "type": "python",
      "request": "launch",
      "module": "strawberry.acquisition.acquire",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      }
    },
    {
      "name": "Validate",
      "type": "python",
      "request": "launch",
      "module": "strawberry.validation.validate",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      }
    },
    {
      "name": "Fact Q Financials",
      "type": "python",
      "request": "launch",
      "module": "strawberry.dimensions.fact_qtr_financials",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      }
    },
    {
      "name": "Dim Stocks",
      "type": "python",
      "request": "launch",
      "module": "strawberry.dimensions.dim_stocks",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      }
    },
    {
      "name": "UI",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "${workspaceFolder}/src/strawberry/ui/app.py"
      ],
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      }
    },
    {
      "name": "Data Viewer",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "${workspaceFolder}/src/strawberry/ui/data_viewer.py"
      ],
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src"
      }
    },
    {
      "name": "Prefect Server",
      "type": "python",
      "request": "launch",
      "module": "prefect.cli",
      "args": [
        "server",
        "start",
        "--port",
        "4200"
      ],
      "console": "integratedTerminal",
      "env": {
        "PREFECT_HOME": "${workspaceFolder}\\.prefect"
      },
      "justMyCode": true
    },
    {
      "name": "Prefect Flow",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/strawberry/prefect_stocks/flow.py",
      "console": "integratedTerminal",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/src",
        "PREFECT_API_URL": "http://127.0.0.1:4200/api"
      },
      "justMyCode": true
    }
  ],
  "compounds": [
    {
      "name": "Server + Flow",
      "configurations": [
        "Prefect Server",
        "Prefect Flow"
      ]
    }
  ]
}